#include <iostream>
#include <cstdio>
#include <algorithm>
#include <vector>
using namespace std;

int a[101];
int b[101];
vector<pair<int, int>> vec;

bool compare(int a, int b)
{
		return a < b;
}
bool compare2 (int a, int b)
{
	return a > b;
}

int main(void)
{
	int num;
	cin >> num;
	for (int i = 0; i < num; i++)
	{
		cin >> a[i];
	}
	for (int i = 0; i < num; i++)
	{
		cin >> b[i];
	}
	sort(a, a+num, compare);
	sort(b, b + num, compare2);
	int result = 0;
	for (int i = 0; i < num; i++)
	{
		result += a[i] * b[i];
	}
	printf("%d", result);
}
